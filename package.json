{
  "name": "mdeck",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/dompurify": "2.0.3",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "25.1.4",
    "@types/markdown-it": "10.0.2",
    "@types/node": "13.9.3",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.3",
    "@types/react-helmet": "5.0.15",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "5.0.1",
    "@types/testing-library__jest-dom": "5.0.2",
    "@types/webpack-env": "1.15.1",
    "cross-env": "7.0.2",
    "dompurify": "2.0.12",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "fontfaceobserver": "2.1.0",
    "husky": "4.2.5",
    "jest-styled-components": "7.0.2",
    "lint-staged": "10.0.8",
    "markdown-it": "11.0.0",
    "markdown-it-task-lists": "2.1.1",
    "prettier": "2.0.5",
    "react": "16.13.0",
    "react-app-polyfill": "1.0.6",
    "react-dom": "16.13.0",
    "react-helmet-async": "1.0.4",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "react-test-renderer": "16.13.0",
    "sanitize.css": "11.0.0",
    "serve": "11.3.2",
    "shelljs": "0.8.3",
    "styled-components": "5.0.1",
    "stylelint": "13.2.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.8.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:prod": "build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,jsx,ts,tsx",
    "lint": "yarn eslint src",
    "lint:fix": "yarn eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "format": "prettier --write src/**/*.ts{,x}",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn eslint -- --fix"
    ],
    "*.{md,json}": [
      "yarn prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@storybook/addon-actions": "6.0.3",
    "@storybook/addon-essentials": "6.0.3",
    "@storybook/addon-links": "6.0.3",
    "@storybook/node-logger": "6.0.3",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "6.0.3",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "react-is": "16.13.1"
  }
}
